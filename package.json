{
  "name": "frontend",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev astro dev",
    "build": "astro build",
    "astro": "astro",
    "format": "pnpm exec prettier . --write",
    "wrangler:types": "wrangler types src/worker.d.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply prod-d1 --local",
    "db:studio": "drizzle-kit studio",
    "preview": "wrangler dev",
    "deploy": "wrangler deploy",
    "wdev": "pnpm run build && pnpm run db:migrate:local && pnpm run preview",
    "wdev-host": "pnpm run build && S3_BUCKET=minio pnpm run db:migrate:local && wrangler dev --ip 127.0.0.1 --port 4321",
    "minio": "./minio server data"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.8",
    "@auth/core": "^0.37.4",
    "@libsql/client": "^0.15.15",
    "astro": "^5.13.7",
    "auth-astro": "^4.2.0",
    "drizzle-orm": "^0.44.5",
    "encoding-japanese": "^2.2.0",
    "sass": "^1.92.1"
  },
  "devDependencies": {
    "@auth/d1-adapter": "^1.10.0",
    "@auth/drizzle-adapter": "^1.10.0",
    "@aws-sdk/client-s3": "^3.888.0",
    "@iconify-json/pixelarticons": "^1.2.2",
    "@types/encoding-japanese": "^2.2.1",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "drizzle-kit": "^0.31.4",
    "drizzle-seed": "^0.3.1",
    "jszip": "^3.10.1",
    "memoize": "^10.1.0",
    "nanoid": "^5.1.5",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "tsx": "^4.20.5",
    "unplugin-icons": "^22.3.0",
    "wrangler": "^4.37.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "auth-astro@4.2.0": "patches/auth-astro.patch"
    }
  }
}